{
    "builds": [
      { "src": "manage.py", "use": "@vercel/python" },
      { 
        "src": "ProyectoFinal/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      {
        "src": "/(.*)",
        "dest": "ProyectoFinal/wsgi.py"
      }
    ]
  }
  